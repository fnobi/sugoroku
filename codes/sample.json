{
	states: {
		initial: {
			x: 50, y: 100,
			elementSwitch: {
				'#section1': false,
				'#section2': false
			}
		},
		state_a: {
			x: 100, y: 300,
			elementSwitch: {
				'#section1': true,
				'#section2': false
			}
		},
		state_b: {
			x: 300, y: 300,
			elementSwitch: {
				'#section1': false,
				'#section2': true
			}
		}

	},
	conditions: {
		direct: {
			type: 'timeout',
			ms: 0
		},
		wait_2s: {
			type: 'timeout',
			ms: 2000
		}
	},
	transitions: [{
		from: '/initial',
		to: '/state_a',
		condition: 'direct'
	}, {
		from: '/state_a',
		to: '/state_b',
		condition: 'wait_2s'
	}, {
		from: '/state_b',
		to: '/state_a',
		condition: 'wait_2s'
	}]
}